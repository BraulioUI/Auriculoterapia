<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.SettingsActivity">

    <TextView
        android:id="@+id/tv_settingNombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginTop="111dp"
        android:text="Nombre:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_settingApellido"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:layout_marginTop="18dp"
        android:text="Apellido:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_settingNombre" />

    <TextView
        android:id="@+id/tv_settingFecha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:layout_marginTop="29dp"
        android:text="Fecha de naciemiento:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_settingApellido" />

    <TextView
        android:id="@+id/tv_settingGenero"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="34dp"
        android:text="Sexo:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_settingFecha" />

    <TextView
        android:id="@+id/tv_settingCorreo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:layout_marginTop="30dp"
        android:text="Correo:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_settingGenero" />

    <TextView
        android:id="@+id/tv_settingContrasena"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="21dp"
        android:layout_marginTop="38dp"
        android:text="Contraseña:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_settingCorreo" />

    <TextView
        android:id="@+id/tv_settingKeyWord"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="21dp"
        android:layout_marginTop="33dp"
        android:text="Palabra Clave:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_settingContrasena" />

    <TextView
        android:id="@+id/tv_settingLogOut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="46dp"
        android:layout_marginEnd="312dp"
        android:layout_marginBottom="237dp"
        android:text="Cerrar sesión"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_settingKeyWord" />

    <TextView
        android:id="@+id/tv_settingNombre2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="110dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/tv_settingNombre"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_settingApellido2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/tv_settingApellido"
        app:layout_constraintTop_toBottomOf="@+id/tv_settingNombre2" />

    <TextView
        android:id="@+id/tv_settingFecha2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:inputType="date"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/tv_settingFecha"
        app:layout_constraintTop_toBottomOf="@+id/tv_settingApellido2" />

    <TextView
        android:id="@+id/tv_settingGenero2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="35dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/tv_settingGenero"
        app:layout_constraintTop_toBottomOf="@+id/tv_settingFecha2" />

    <TextView
        android:id="@+id/tv_settingCorreo2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="30dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/tv_settingCorreo"
        app:layout_constraintTop_toBottomOf="@+id/tv_settingGenero2" />

    <TextView
        android:id="@+id/tv_settingContrasena2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="37dp"
        android:inputType="textPassword"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/tv_settingContrasena"
        app:layout_constraintTop_toBottomOf="@+id/tv_settingCorreo2" />

    <TextView
        android:id="@+id/tv_settingKeyWord2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:inputType="textPassword"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/tv_settingKeyWord"
        app:layout_constraintTop_toBottomOf="@+id/tv_settingContrasena2" />

    <TextView
        android:id="@+id/tv_settingModificar1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="152dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="32dp"
        android:text="Modificar"
        app:layout_constraintBottom_toTopOf="@+id/tv_settingKeyWord2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_settingContrasena2" />

    <TextView
        android:id="@+id/tv_settingModicar2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="138dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="56dp"
        android:text="Modificar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_settingKeyWord2"
        app:layout_constraintTop_toBottomOf="@+id/tv_settingModificar1" />

</androidx.constraintlayout.widget.ConstraintLayout>