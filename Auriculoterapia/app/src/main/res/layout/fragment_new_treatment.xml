<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/et_otros"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="113dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="26dp"
        android:ems="10"
        android:gravity="start|top"
        android:hint="otros"
        android:inputType="textMultiLine"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/tv_otros"
        app:layout_constraintTop_toBottomOf="@+id/btn_InsertarImagen" />

    <EditText
        android:id="@+id/et_peso"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="26dp"
        android:ems="10"
        android:hint="70.0"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/tv_peso"
        app:layout_constraintTop_toBottomOf="@+id/tv_resultEdad" />

    <TextView
        android:id="@+id/tv_otros"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="33dp"
        android:text="Otros:"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_otros"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/et_altura"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="26dp"
        android:ems="10"
        android:hint="1.70"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/tv_altura"
        app:layout_constraintTop_toBottomOf="@+id/et_peso" />

    <Button
        android:id="@+id/btn_registrarTratamiento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Registrar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_otros" />

    <TextView
        android:id="@+id/tv_edad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:text="Edad:"
        app:layout_constraintBaseline_toBaselineOf="@+id/tv_resultEdad"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tv_sintomas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:text="SÃ­ntomas:"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_Sintomas"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tv_peso"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:text="Peso (kg):"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_peso"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tv_altura"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:text="Altura (m):"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_altura"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tv_examinarFoto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:text="Examinar:"
        app:layout_constraintBaseline_toBaselineOf="@+id/btn_InsertarImagen"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_InsertarImagen"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="46dp"
        android:layout_marginEnd="70dp"
        android:text="Subir Foto"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_Sintomas" />

    <EditText
        android:id="@+id/et_Sintomas"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="85dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="26dp"
        android:ems="10"
        android:gravity="start|top"
        android:hint="Dolor..."
        android:inputType="textMultiLine"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/tv_sintomas"
        app:layout_constraintTop_toBottomOf="@+id/et_altura" />

    <TextView
        android:id="@+id/tv_resultEdad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="115dp"
        android:layout_marginTop="24dp"
        android:text="result"
        app:layout_constraintStart_toEndOf="@+id/tv_edad"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_urlImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/btn_InsertarImagen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_examinarFoto"
        app:layout_constraintTop_toBottomOf="@+id/et_Sintomas" />

</androidx.constraintlayout.widget.ConstraintLayout>